<h1>Listing events</h1>

<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>End time</th>
      <th>Description</th>
      <th>Pooped</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.start_time %></td>
        <td><%= event.end_time %></td>
        <td><%= event.description %></td>
        <td><%= event.pooped %></td>
        <td><%= event.comments %></td>
        
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_day_event_path(params[:day_id]) %>
